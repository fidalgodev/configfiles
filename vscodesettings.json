{
  "terminal.integrated.shell.osx": "/bin/zsh",
  "editor.fontFamily": "Dank Mono, Operator Mono",
  "editor.minimap.enabled": false,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "editor.fontSize": 16,
  // "editor.fontSize": 20,
  // "editor.lineHeight":0,
  "editor.lineHeight":30,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.tabSize": 2,
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "solid",
  "editor.wordWrap": "on",
  "editor.suggestSelection": "first",
  "zenMode.centerLayout": false,
  "editor.renderWhitespace": "boundary",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  // ENABLE FORMAT ON SAVE WITH PRETTIER FOR EVERYTHING OTHER THAN JS, JS REACT, TS and TS REACT
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  // "[javascript]": {
  //   "editor.formatOnSave": false,
  // },
  // "[javascriptreact]": {
  //   "editor.formatOnSave": false,
  // },
  // "[typescript]": {
  //   "editor.formatOnSave": false,
  // },
  // "[typescriptreact]": {
  //   "editor.formatOnSave": false,
  // },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.rendererType": "dom",
  "editor.colorDecorators": false,
  "terminal.integrated.fontSize": 16,
  "window.zoomLevel": 0,
  "prettier.singleQuote": true,
  "workbench.editor.tabSizing": "shrink",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "terminal.integrated.fontFamily": "Dank Mono",
  "workbench.colorTheme": "morgan.codes",
  "breadcrumbs.enabled": true,
  "workbench.fontAliasing": "auto",
  "sync.gist": "784b3c5a00a98450c62ac9abd64bc5ee",
}

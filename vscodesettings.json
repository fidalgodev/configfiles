{
  "terminal.integrated.shell.osx": "/bin/zsh",
  "editor.fontFamily": "Dank Mono, Operator Mono",
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80
  ],
  "extensions.ignoreRecommendations": false,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "editor.fontSize": 18,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.tabSize": 2,
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "solid",
  "editor.wordWrap": "on",
  "sync.gist": "9108b85452251bf9fef588beea6f342c",
  "editor.suggestSelection": "first",
  "zenMode.centerLayout": false,
  "editor.renderWhitespace": "boundary",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  // ENABLE FORMAT ON SAVE WITH PRETTIER FOR EVERYTHING OTHER THAN JS, JS REACT, TS and TS REACT
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  // "[javascript]": {
  //   "editor.formatOnSave": false,
  // },
  // "[javascriptreact]": {
  //   "editor.formatOnSave": false,
  // },
  // "[typescript]": {
  //   "editor.formatOnSave": false,
  // },
  // "[typescriptreact]": {
  //   "editor.formatOnSave": false,
  // },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    // "source.fixAll.stylelint": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.rendererType": "dom",
  "editor.colorDecorators": false,
  "terminal.integrated.fontSize": 18,
  "window.zoomLevel": 0,
  "prettier.singleQuote": true,
  "workbench.editor.tabSizing": "shrink",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.lineHeight": 30,
  "terminal.integrated.fontFamily": "Dank Mono",
  "workbench.fontAliasing": "auto",
  "workbench.colorTheme": "CodeSandbox",
}
